// This is your Prisma schema file
// Learn more at https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URI")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        Int     @id @default(autoincrement())
  title     String
  completed Boolean @default(false)
} 